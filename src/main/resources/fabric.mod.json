{
  "schemaVersion": 1,
  "id": "${modId}",
  "version": "${modVersion}",
  "name": "Better Scaffolding",
  "description": "Scaffolding, but \"realistic\"",
  "authors": [
    "nea89",
    "einsJannis"
  ],
  "contact": {
    "sources": "https://github.com/romangraef/betterscaffolding"
  },
  "license": "MIT",
  "icon": "assets/betterscaffolding/logo.png",
  "environment": "*",
  "entrypoints": {
    "main": [
      {
        "adapter": "kotlin",
        "value": "com.romangraef.betterscaffolding.BetterScaffolding"
      }
    ],
    "client": [
      {
        "adapter": "kotlin",
        "value": "com.romangraef.betterscaffolding.BetterScaffoldingClient"
      }
    ],
    "modmenu": [
      {
        "adapter": "kotlin",
        "value": "com.romangraef.betterscaffolding.interop.ModmenuInterop"
      }
    ]
  },
  "mixins": [
    "betterscaffolding.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=0.11.3",
    "fabric": "*",
    "minecraft": "1.17.x",
    "java": ">=16"
  },
  "suggests": {
  },
  "custom": {
    "fabric-language-kotlin": ">=1.6.1+kotlin.1.5.10"
  }
}